{
  "expo": {
    "name": "UBinzo",
    "slug": "ubinzo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/black-logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logos/black-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins":[
     [ "expo-media-library",{
      "photosPermission":"Allow UBinzo to access your photos.",
      "savePhotosPermission":"Allow UBinzo to save photos",
      "isAccessMediaLocationEnabled":true
    }]
    ]
  }
}
